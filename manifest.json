{
    "manifest_version": 2,

    "name": "Seawall",
    "description": "This extension gets around Seacoast Online paywall.",
    "version": "0.0.1",

    "permissions": [
        "http://www.seacoastonline.com/",
        "http://*.seacoastonline.com/",
        "cookies"
    ],

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Seawall"
    },

    "background": {
        "scripts": ["events.js", "injection.js"],
        "persistent": false
    }


}